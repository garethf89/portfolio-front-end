  "scripts": {
    "dev": "next dev",
    "build": "yarn prepare && next build",
    "start": "next start",
    "prepare": "panda codegen",
    "test": "vitest",
    "setup": "node ./bin/setup.js",
    "typecheck": "tsc --noEmit",
    "lint:js": "eslint 'src/**/*.{tsx,ts}'",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "check-pretty": "prettier --list-different {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.tsx\"",
    "storybook": "yarn prepare && storybook dev -p 6006",
    "build-storybook": "yarn prepare && storybook build",
    "analyze-size": "source-map-explorer public/app*.tsx",
    "codegen": "DOTENV_CONFIG_PATH=.env.development graphql-codegen --config codegen.ts -r dotenv/config",
    "previewSetup": "cp -f preview.toml netlify.tomyarn",
    "contentful-export": "contentful space export --config contentful/export-config.json --management-token %CONTENTFUL_MANAGEMENT_PAT% --space-id %CONTENTFUL_SPACE_ID%",
    "contentful-typegen": "cf-content-types-generator contentful-export.json --v10 --out @types/generated --response   && prettier @types/generated --write"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
